{
  "name": "blog-editor",
  "version": "1.4.4",
  "author": "Alex Zhao",
  "main": "./dist/electron/main.js",
  "license": "UNLICENSED",
  "build": {
    "appId": "me.alexzhao.blogeditor",
    "productName": "Editor",
    "win": {
      "target": "zip"
    },
    "files": [
      "dist/editor/*",
      "dist/electron/main.js"
    ]
