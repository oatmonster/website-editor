    "@angular/language-service": "~9.1.11",
    "@types/node": "^12.11.1",
    "@types/prismjs": "^1.16.0",
    "electron": "8.2.2",
    "electron-builder": "^22.6.0",
    "ts-node": "~8.3.0",
    "typescript": "~3.7.5"
  }
